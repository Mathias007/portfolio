{
    "name": "mathias-portfolio-server",
    "version": "1.0.0",
    "description": "Serwer backendowy dla projektu Mathias Portfolio, napisany z wykorzystniem Node.js we frameworku Express, z połączeniem z bazą danych MongoDB.",
    "main": "src/index.js",
    "license": "MIT",
    "scripts": {
        "start": "nodemon --exec babel-node src/index.js",
        "build": "babel src --out-dir dist",
        "serve": "node dist/index.js"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "body-parser": "^1.19.0",
        "core-js": "^3.9.1",
        "dotenv": "^9.0.2",
        "express": "4.17.1",
        "mongoose": "^5.12.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/node": "^7.13.13",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-export-default-from": "^7.12.13",
        "@babel/plugin-transform-async-to-generator": "^7.13.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.13.12",
        "nodemon": "2.0.3"
    }
}
